////Read numbers from the keyboard until zero is entered. Determine how many of them were even.

#include <iostream>
using namespace std;
int main()
{
    int k=0,i,v[110],ok=0;
    for(i=0;i<=100;i++)
    {
        cin>>v[i];
        if(v[i]==0)
            ok=1;
        if(ok==0)
        {
            if(v[i]%2==0)
                k++;
        }
    }
    if(k==0)
        cout<<"NU EXISTA";
    else cout<<k;
    return 0;
}
