// Read an array with $n$ natural numbers. Replace the last prime element in the array with 0, then display the modified array.

#include <iostream>
using namespace std;
int prim(int n)
{
    int d=2;
    if(n<=1) return 0;
    for(d=2;d*d<=n;d++)
    {
        if(n%d==0)
            return 0;
    }
    return 1;
}
int main()
{
    int n,i,v[1001];
    cin>>n;
    for(i=0;i<n;i++)
        cin>>v[i];
    for(i=n-1;i>0;i--)
    {   
        if(prim(v[i])==1)
        {   
             v[i]=0;
             break;
        }
    }
    for(i=0;i<n;i++)
        cout<<v[i]<<' ';
    return 0;
}
