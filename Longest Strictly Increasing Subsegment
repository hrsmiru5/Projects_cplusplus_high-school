// Given an array of $n$ elements, determine the longest contiguous subsegment of elements that are strictly increasing.

#include <iostream>
#include <fstream>
using namespace std;
int main()
{ 
    ifstream fin("secvcresc.in");
    ofstream fout("secvcresc.out");
    int lc=1,st,dr,lmax=0,x,y,i,n;
    fin>>n;
    fin>>x;
    for(i=2;i<=n;i++)
    {fin>>y;
     if(x<y)
         lc++;
     else
        {
         if(lc>lmax)
         {    lmax=lc;
              dr=i-1;
              st=dr-lmax+1;
         }
         lc=1;}
     x=y;}
    if(lc>lmax)
    {
        lmax=lc;
        dr=i-1;
        st=dr-lmax+1;
    }
    fout<<st<<' '<<dr;
    fin.close();
    fout.close();
    return 0;}
      
          
