// Read $n$ natural numbers representing the heights of $n$ buildings. The requirement is to create an architectural project where the buildings are sorted in descending order based on their heights.

#include <iostream>
using namespace std;
int main()
{
    int v[1001],i,j,aux,n;
    cin>>n;
    for(i=0;i<n;i++)
        cin>>v[i];
    for(i=0;i<n-1;i++)
    {
        for(j=i;j<n;j++)
        {
            if(v[i]<v[j])
            {
                aux=v[i];
                v[i]=v[j];
                v[j]=aux;
            }
        }
    }
    for(i=0;i<n;i++)
        cout<<v[i]<<' ';
    return 0;
}
